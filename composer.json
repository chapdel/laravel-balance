{
    "name": "chapdel/credits",
    "description": "A credit system for Laravel.",
    "keywords": [
        "balance",
        "laravel",
        "credit-system"
    ],
    "homepage": "https://github.com/chapdel/credits",
    "license": "MIT",
    "authors": [
        {
            "name": "George Tudor",
            "email": "georgebitq@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.4",
        "spatie/laravel-package-tools": "^1.14.0",
        "illuminate/contracts": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "Chapdel\\Credit\\": "src/",
            "Chapdel\\Credit\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chapdel\\Credit\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chapdel\\Credit\\CreditServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
